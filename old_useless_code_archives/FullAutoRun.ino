#include <Keyboard.h>;
#include <Mouse.h>;
void setup(){
  delay(3000);
  Mouse.begin();
  delay(50);
  Keyboard.begin();
  delay(50);
}
void loop(){
  delay(100);
  Keyboard.press('w');
  delay(100);
  Keyboard.press(KEY_LEFT_SHIFT);
  delay(20000);
  Keyboard.releaseAll();
  delay(150);
  Keyboard.press('w');
  delay(20000);
  Keyboard.release('w');
  delay(100);
  turnBack();
  delay(100);
}
